<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.76-ea"
            fx:controller="com.waicool20.kaga.controllers.GeneralTabController">
    <children>
        <GridPane>
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="294.0" minWidth="10.0"
                                   prefWidth="138.0"/>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="531.0" minWidth="10.0"
                                   prefWidth="462.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0"
                                vgrow="SOMETIMES"/>
                <RowConstraints maxHeight="45.0" minHeight="45.0" prefHeight="45.0"
                                vgrow="SOMETIMES"/>
                <RowConstraints minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES"/>
                <RowConstraints minHeight="45.0" prefHeight="45.0" vgrow="SOMETIMES"/>
            </rowConstraints>
            <children>
                <HBox alignment="CENTER">
                    <children>
                        <Label text="Program:"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" GridPane.rowIndex="1">
                    <children>
                        <Label text="Recovery Method:"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1">
                    <children>
                        <TextField fx:id="programTextField"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="1">
                    <children>
                        <ChoiceBox fx:id="recoveryMethodChoiceBox" prefWidth="150.0"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" GridPane.rowIndex="4">
                    <children>
                        <Label text="Sleep Cycle:"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" GridPane.rowIndex="3">
                    <children>
                        <Label text="Paranoia:"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="4">
                    <children>
                        <Spinner fx:id="sleepCycleSpinner" editable="true"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="3">
                    <children>
                        <Spinner fx:id="paranoiaSpinner" editable="true"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" GridPane.rowIndex="5">
                    <children>
                        <Label text="Sleep Modifier:"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="5">
                    <children>
                        <Spinner fx:id="sleepModifierSpinner" editable="true"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" GridPane.rowIndex="2">
                    <children>
                        <Label text="Basic Recovery:"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="2">
                    <children>
                        <CheckBox fx:id="basicRecoveryCheckBox" mnemonicParsing="false"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" GridPane.rowIndex="6">
                    <children>
                        <Label text="Sikuli Script Jar:"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" GridPane.rowIndex="7">
                    <children>
                        <Label text="Kancolle Auto Root:"/>
                    </children>
                </HBox>
                <Label fx:id="sikuliScriptJarPathLabel" GridPane.columnIndex="1"
                       GridPane.rowIndex="6"/>
                <Label fx:id="kancolleAutoRootPathLabel" GridPane.columnIndex="1"
                       GridPane.rowIndex="7"/>
            </children>
        </GridPane>
    </children>
</AnchorPane>
